{
  "_version": "1.65.0",
  "sap.app": {
    "id": "visitors",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/catalog/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "sourceTemplate": {
      "id": "@sap/generator-fiori:worklist",
      "version": "1.18.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.136.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "visitors.i18n.i18n"
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "Data_RoomsList",
          "target": "Data_RoomsList"
        },
        {
          "pattern": "Data_Rooms({key}):?query:",
          "name": "Data_RoomsObjectPage",
          "target": "Data_RoomsObjectPage"
        }
      ],
      "targets": {
        "Data_RoomsList": {
          "type": "Component",
          "id": "Data_RoomsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Data_Rooms",
              "variantManagement": "Page",
              "navigation": {
                "Data_Rooms": {
                  "detail": {
                    "route": "Data_RoomsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "Data_RoomsObjectPage": {
          "type": "Component",
          "id": "Data_RoomsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "entitySet": "Data_Rooms"
            }
          }
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.viewExtensions": {
          "sap.suite.ui.generic.template.ListReport.view.ListReport": {
            "AfterListReportHeader": {
              "className": "sap.ui.core.Fragment",
              "fragmentName": "visitors.ext.fragment.CustomActions",
              "type": "XML"
            }
          }
        }
      }
    },
    "resources": {
      "css": []
    }
  }

}
